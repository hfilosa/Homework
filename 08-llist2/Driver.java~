public class Driver{
		public static void main(String[] args) {
		    LList l = new LList();
		    l.add("hello");
		    l.add("world");
		    for (int i=0;i<5;i++){
			l.add(""+i);
		    }
		    System.out.println(l);
		    System.out.println("Zeroth Link: "+l.find(0));
		    System.out.println("Third Link: "+l.find(3));
		    System.out.println("Tenth link: "+l.find(10));
		    System.out.println("\n---------------\n");
		    l.insert(0,"a");
		    System.out.println("'a' inserted as zeroth link");
		    System.out.println(l+"\n");
		    l.insert(5,"b");
		    System.out.println("'b' inserted as fifth link");
		    System.out.println(l+"\n");
		    l.insert(9,"c");
		    System.out.println("'c' inserted as ninth link");
		    System.out.println("Method will not go past the end of the links");
		    System.out.println(l);


		}
}
